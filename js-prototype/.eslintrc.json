{
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "node": true
  },
  "extends": ["eslint:recommended"],
  "plugins": ["import"],
  "rules": {
    "max-params": ["warn", 4],
    "no-console": "off",
    "no-constant-condition": "off"
  }
}
